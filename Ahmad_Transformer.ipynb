    "import torch\n",
    "import os\n",
    "\n",
    "# Ensure the environment variable is set to enable CUDA assertions (if running on CUDA)\n",
    "os.environ['TORCH_USE_CUDA_DSA'] = '1'\n",
    "\n",
    "# Turn on anomaly detection for debugging\n",
    "torch.autograd.set_detect_anomaly(True)\n",
    "\n",
    "# Define the optimizer\n",
    "optimizer = torch.optim.AdamW(model.parameters(), lr=1e-3)  # Starting with a learning rate of 1e-3\n",
    "\n",
    "# Function to check model weights and gradients\n",
    "def check_gradients():\n",
    "    for name, param in model.named_parameters():\n",
    "        if param.grad is not None:\n",
    "            pass  # Placeholder for future functionality\n",
    "\n",
    "# Function to check if data is valid\n",
    "def check_data_validity(data):\n",
    "    if (data < 0).any() or (data >= vocab_size).any():\n",
    "        return False\n",
    "    return True\n",
    "\n",
    "def get_batch(split, device):\n",
    "    # Simulating a batch loading function\n",
    "    batch_size = 64\n",
    "    seq_len = 128\n",
    "    # This should return indices of tokens (your data pipeline)\n",
    "    X = torch.randint(0, vocab_size, (batch_size, seq_len), dtype=torch.long, device=device)\n",
    "    Y = torch.randint(0, vocab_size, (batch_size, seq_len), dtype=torch.long, device=device)\n",
    "\n",
    "    if not check_data_validity(X) or not check_data_validity(Y):\n",
    "        return None, None  # Return None to skip this batch\n",
    "\n",
    "    return X, Y\n",
    "\n",
    "# Function to estimate loss during training\n",
    "@torch.no_grad()\n",
    "def estimate_loss():\n",
    "    out = {}\n",
    "    model.eval()\n",
    "    for split in ['train', 'val']:\n",
    "        losses = torch.zeros(eval_iters, device=device)  # Ensure losses tensor is on the correct device\n",
    "        for k in range(eval_iters):\n",
    "            X, Y = get_batch(split, device)\n",
    "            if X is None or Y is None:\n",
    "                continue  # Skip the invalid batches\n",
    "\n",
    "            X, Y = X.to(device), Y.to(device)  # Ensure data is on the same device\n",
    "\n",
    "            logits, loss = model(X, Y)\n",
    "\n",
    "            if torch.isnan(logits).any() or torch.isinf(logits).any():\n",
    "                continue\n",
    "\n",
    "            if torch.isnan(loss).any() or torch.isinf(loss).any():\n",
    "                continue\n",
    "\n",
    "            losses[k] = loss.item()\n",
    "        out[split] = losses.mean()\n",
    "    model.train()\n",
    "    return out\n",
    "\n",
    "# Function to train the model\n",
    "def train():\n",
    "    for iter in range(max_iter):\n",
    "        if iter % eval_interval == 0 or iter == max_iter - 1:\n",
    "            losses = estimate_loss()\n",
    "            print(f\"step {iter}: train loss {losses['train']:.4f}, val loss {losses['val']:.4f}\")  # Print the loss\n",
    "\n",
    "        xb, yb = get_batch('train', device)\n",
    "        if xb is None or yb is None:\n",
    "            continue  # Skip invalid batches\n",
    "\n",
    "        xb, yb = xb.to(device), yb.to(device)\n",
    "\n",
    "        logits, loss = model(xb, yb)\n",
    "        optimizer.zero_grad(set_to_none=True)\n",
    "        loss.backward()\n",
    "\n",
    "        check_gradients()\n",
    "\n",
    "        optimizer.step()\n",
    "\n",
    "# Run the training process\n",
    "train()\n",
    "\n",
    "# Generating output with the model after training\n",
    "context = torch.zeros((1, 1), dtype=torch.long, device=device)  # Ensure context tensor is on the same device\n",
    "generated_tokens = model.generate(context, max_new_tokens=500)\n",
    "decoded_output = bpe.decode(generated_tokens[0].tolist())  # Ensure the generated tokens are decoded correctly\n",
    "print(decoded_output)\n"
