    "import torch\n",
    "import torch.nn as nn\n",
    "\n",
    "ff_mult = 4\n",
    "\n",
    "class FeedForward(nn.Module):\n",
    "    def __init__(self, n_embd, device):\n",
    "        \"\"\"\n",
    "        Initializes the FeedForward neural network.\n",
    "\n",
    "        Parameters:\n",
    "        - n_embd (int): The dimensionality of the input and output embeddings.\n",
    "        - device: The device (either 'cpu' or 'cuda') to store tensors.\n",
    "        \"\"\"\n",
    "        super().__init__()\n",
    "        self.device = device\n",
    "\n",
    "        # Linear layers for transformation\n",
    "        self.w1 = nn.Linear(n_embd, ff_mult * n_embd)  # First linear transformation\n",
    "        self.w2 = nn.Linear(ff_mult * n_embd // 2, n_embd)  # Second linear transformation\n",
    "        self.w3 = nn.Linear(n_embd, ff_mult * n_embd // 2) # Third linear transformation for residual connections\n",
    "\n",
    "    def forward(self, x):\n",
    "        \"\"\"\n",
    "        Forward pass for the FeedForward network.\n",
    "\n",
    "        Parameters:\n",
    "        - x (Tensor): Input tensor of shape (B, T, n_embd), where B is batch size, T is sequence length.\n",
    "\n",
    "        Returns:\n",
    "        - Tensor: Output tensor of the same shape as input after applying the feedforward operations.\n",
    "        \"\"\"\n",
    "        # Ensure the input tensor is on the correct device\n",
    "        x = x.to(self.device)\n",
    "\n",
    "        # 1. Apply the first linear transformation (self.w1).\n",
    "        z = self.w1(x)  # Shape: (B, T, ff_mult * n_embd)\n",
    "\n",
    "        # 2. Apply the Swish activation (Sigmoid Linear Unit).\n",
    "        swish = z * torch.sigmoid(z)  # Element-wise Swish activation\n",
    "\n",
    "        # 3. Apply the third linear transformation (self.w3) for gating.\n",
    "        g = self.w3(x)  # Shape: (B, T, ff_mult * n_embd // 2)\n",
    "\n",
    "        # 4. Apply element-wise multiplication for gating.\n",
    "        gated = swish[:, :, :g.size(-1)] * g  # Gated interaction\n",
    "\n",
    "        # 5. Apply the second linear transformation (self.w2).\n",
    "        out = self.w2(gated)  # Shape: (B, T, n_embd)\n",
    "\n",
    "        return out\n"
