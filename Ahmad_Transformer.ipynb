    "import torch\n",
    "\n",
    "class BPE:\n",
    "    def __init__(self, vocab_size):\n",
    "        \"\"\"\n",
    "        Initializes the BPE tokenizer.\n",
    "\n",
    "        Parameters:\n",
    "        - vocab_size (int): The maximum size of the vocabulary.\n",
    "        \"\"\"\n",
    "        self.vocab_size = vocab_size\n",
    "        self.vocab = {}\n",
    "        self.bpe_codes = {}\n",
    "\n",
    "    def fit(self, text):\n",
    "        vocab = {}\n",
    "        for word in text.split():\n",
    "            word = list(word) + ['</w>']\n",
    "            vocab[\" \".join(word)] = vocab.get(\" \".join(word), 0) + 1\n",
    "\n",
    "        # Merge pairs until the desired vocab size is reached\n",
    "        while len(self.bpe_codes) < self.vocab_size:\n",
    "            pairs = self._get_pairs(vocab)\n",
    "            if not pairs:\n",
    "                break\n",
    "\n",
    "            # Find the most frequent pair\n",
    "            most_frequent_pair = max(pairs, key=pairs.get)\n",
    "            self.bpe_codes[most_frequent_pair] = len(self.bpe_codes)\n",
    "\n",
    "            # Merge the pair in the vocabulary\n",
    "            vocab = self._merge_pair(vocab, most_frequent_pair)\n",
    "\n",
    "        self.vocab = list(vocab.keys())\n",
    "\n",
    "    def encode(self, text):\n",
    "        \"\"\"\n",
    "        Encodes the input text into a sequence of BPE tokens.\n",
    "        \"\"\"\n",
    "        tokens = [\" \".join(list(word) + ['</w>']) for word in text.split()]\n",
    "        for pair, _ in sorted(self.bpe_codes.items(), key=lambda x: x[1]):\n",
    "            pair_str = \" \".join(pair)\n",
    "            replacement = \"\".join(pair)\n",
    "            tokens = [token.replace(pair_str, replacement) for token in tokens]\n",
    "\n",
    "        return [token for token in \" \".join(tokens).split() if token != '</w>']\n",
    "\n",
    "    def decode(self, encoded):\n",
    "        \"\"\"\n",
    "        Decodes a sequence of BPE tokens back into the original text.\n",
    "        \"\"\"\n",
    "        text = []\n",
    "        for token in encoded:\n",
    "            if token.endswith(\"</w>\"):\n",
    "                text.append(token.replace(\"</w>\", \"\"))  # Remove </w>\n",
    "                text.append(\" \")  # Add a space after the word\n",
    "            else:\n",
    "                text.append(token)  # Keep the subword as is\n",
    "\n",
    "        # Join everything into a single string\n",
    "        result = \"\".join(text)\n",
    "\n",
    "        # Strip excessive spaces (from trailing spaces added above)\n",
    "        return result.strip()\n",
    "\n",
    "    def _get_pairs(self, vocab):\n",
    "        \"\"\"\n",
    "        Returns the frequency of all adjacent symbol pairs in the vocab.\n",
    "        \"\"\"\n",
    "        pairs = {}\n",
    "        for word, freq in vocab.items():\n",
    "            symbols = word.split()\n",
    "            for i in range(len(symbols) - 1):\n",
    "                pair = (symbols[i], symbols[i + 1])\n",
    "                pairs[pair] = pairs.get(pair, 0) + freq\n",
    "        return pairs\n",
    "\n",
    "    def _merge_pair(self, vocab, pair):\n",
    "        \"\"\"\n",
    "        Merges a given pair of symbols in the vocabulary.\n",
    "        \"\"\"\n",
    "        new_vocab = {}\n",
    "        pair_str = \" \".join(pair)\n",
    "        replacement = \"\".join(pair)\n",
    "        for word, freq in vocab.items():\n",
    "            new_word = word.replace(pair_str, replacement)\n",
    "            new_vocab[new_word] = freq\n",
    "        return new_vocab\n",
    "\n",
    "\n",
    "# Initialize BPE tokenizer with a vocabulary size\n",
    "bpe = BPE(vocab_size=100)\n",
    "\n",
    "# Load input text\n",
    "text = open('input.txt', 'r').read()\n",
    "\n",
    "# Fit the BPE tokenizer\n",
    "bpe.fit(text)\n",
    "\n",
    "# Encode the text\n",
    "encoded_text = bpe.encode(text)\n",
    "\n",
    "# Create a token-to-ID mapping based on the vocabulary\n",
    "token_to_id = {token: idx for idx, token in enumerate(bpe.vocab)}\n",
    "\n",
    "# Handle tokens that are not in the vocabulary\n",
    "UNK_TOKEN = \"<unk>\"  # A special token for unknown words\n",
    "UNK_TOKEN_ID = len(token_to_id)  # Assign a new ID for the unknown token\n",
    "token_to_id[UNK_TOKEN] = UNK_TOKEN_ID\n",
    "\n",
    "# Encode the text into token IDs, using the UNK_TOKEN_ID for unknown tokens\n",
    "encoded_token_ids = [\n",
    "    token_to_id.get(token, UNK_TOKEN_ID) for token in encoded_text\n",
    "]\n",
    "\n",
    "# Convert encoded token IDs into tensor\n",
    "data = torch.tensor(encoded_token_ids, dtype=torch.long)\n",
    "\n",
    "# Split the data into training and validation sets\n",
    "n = int(0.9 * len(data))\n",
    "train_data = data[:n]\n",
    "val_data = data[n:]\n",
    "\n",
    "# Decode the encoded token IDs back into text\n",
    "decoded_text = bpe.decode(encoded_text)\n",
    "\n",
    "# Print out the results\n",
    "print(f\"Original text: {text[:100]}\")\n",
    "print(f\"Encoded text: {encoded_text[:100]}\")\n",
