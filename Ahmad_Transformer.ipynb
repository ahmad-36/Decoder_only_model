    "import torch\n",
    "import torch.nn as nn\n",
    "import torch.nn.functional as F\n",
    "\n",
    "class Head(nn.Module):\n",
    "    def __init__(self, head_size, n_embd, block_size, dropout, device):\n",
    "        \"\"\"\n",
    "        Initializes the attention head.\n",
    "\n",
    "        Parameters:\n",
    "        - head_size (int): The size of each attention head.\n",
    "        - n_embd (int): The embedding dimension.\n",
    "        - block_size (int): The block size (max sequence length).\n",
    "        - dropout (float): Dropout rate.\n",
    "        - device: The device (either 'cpu' or 'cuda') to store tensors.\n",
    "        \"\"\"\n",
    "        super().__init__()\n",
    "        self.device = device\n",
    "\n",
    "        self.key = nn.Linear(n_embd, head_size, bias=False)\n",
    "        self.query = nn.Linear(n_embd, head_size, bias=False)\n",
    "        self.value = nn.Linear(n_embd, head_size, bias=False)\n",
    "        self.register_buffer('tril', torch.tril(torch.ones(block_size, block_size)))\n",
    "        self.dropout = nn.Dropout(dropout)\n",
    "\n",
    "        # Correct RoPE initialization: Compute inverse frequencies\n",
    "        inv_freq = 1.0 / (10000 ** (torch.arange(0, head_size, 2).float() / head_size))\n",
    "\n",
    "        pos = torch.arange(block_size).unsqueeze(1)\n",
    "        angles = pos * inv_freq\n",
    "\n",
    "        self.register_buffer('cos', torch.cos(angles).to(device))\n",
    "        self.register_buffer('sin', torch.sin(angles).to(device))\n",
    "\n",
    "    def apply_rope(self, q, k, seq_len):\n",
    "        \"\"\"\n",
    "        Applies Rotary Positional Embeddings (RoPE) to query (q) and key (k).\n",
    "\n",
    "        Parameters:\n",
    "        - q: Tensor of shape (B, T, C) representing the query.\n",
    "        - k: Tensor of shape (B, T, C) representing the key.\n",
    "        - seq_len: The sequence length (T).\n",
    "\n",
    "        Returns:\n",
    "        - q_rot, k_rot: The rotated query and key tensors.\n",
    "        \"\"\"\n",
    "        # Ensure the last dimension of q and k is even\n",
    "        assert q.shape[-1] % 2 == 0, \"Embedding size must be even for RoPE.\"\n",
    "        assert k.shape[-1] % 2 == 0, \"Embedding size must be even for RoPE.\"\n",
    "\n",
    "        d_model = q.shape[-1]  # Dimensionality of embeddings\n",
    "        half_dim = d_model // 2  # Half of embedding size\n",
    "        device = q.device\n",
    "\n",
    "        # Compute rotary frequencies\n",
    "        inv_freq = 1.0 / (10000 ** (torch.arange(0, half_dim, dtype=torch.float32, device=device) / half_dim))\n",
    "\n",
    "        t = torch.arange(seq_len, dtype=torch.float32, device=device).unsqueeze(1)  # Shape: (T, 1)\n",
    "        freqs = torch.matmul(t, inv_freq.unsqueeze(0))  # Shape: (T, half_dim)\n",
    "\n",
    "        # Compute cos and sin\n",
    "        cos = torch.cos(freqs).unsqueeze(0).unsqueeze(0)  # Shape: (1, 1, T, half_dim)\n",
    "        sin = torch.sin(freqs).unsqueeze(0).unsqueeze(0)  # Shape: (1, 1, T, half_dim)\n",
    "\n",
    "        # Reshape q and k for even-odd separation\n",
    "        q1, q2 = q[..., :half_dim], q[..., half_dim:]  # Even and odd parts\n",
    "        k1, k2 = k[..., :half_dim], k[..., half_dim:]  # Even and odd parts\n",
    "\n",
    "        # Apply rotation\n",
    "        q_rot = torch.cat([q1 * cos - q2 * sin, q1 * sin + q2 * cos], dim=-1)\n",
    "        k_rot = torch.cat([k1 * cos - k2 * sin, k1 * sin + k2 * cos], dim=-1)\n",
    "\n",
    "        # Remove the extra singleton dimension\n",
    "        q_rot = q_rot.squeeze(0)  # Remove the first dimension (size 1)\n",
    "        k_rot = k_rot.squeeze(0)  # Remove the first dimension (size 1)\n",
    "\n",
    "        return q_rot, k_rot\n",
    "\n",
    "\n",
    "    def forward(self, x):\n",
    "        \"\"\"\n",
    "        Forward pass of the attention head.\n",
    "\n",
    "        Parameters:\n",
    "        - x (Tensor): The input tensor of shape (B, T, C), where B is batch size, T is sequence length, and C is embedding dimension.\n",
    "\n",
    "        Returns:\n",
    "        - out (Tensor): The output tensor after applying attention and RoPE.\n",
    "        \"\"\"\n",
    "        B, T, C = x.shape  # B: batch size, T: sequence length, C: embedding dimension\n",
    "\n",
    "        # Key, query, and value projections\n",
    "        k = self.key(x)  # Key representations\n",
    "        q = self.query(x)  # Query representations\n",
    "        v = self.value(x)  # Value representations\n",
    "\n",
    "        # print(f\"q shape: {q.shape}\")  # Should be [batch_size, seq_len, head_size]\n",
    "        # print(f\"k shape: {k.shape}\")  # Should be [batch_size, seq_len, head_size]\n",
    "\n",
    "        # Apply RoPE to query and key\n",
    "        q, k = self.apply_rope(q, k, T)  # Apply RoPE rotation\n",
    "\n",
    "        # print(f\"q shape after applying RoPE: {q.shape}\")  # Should be [batch_size, seq_len, head_size]\n",
    "        # print(f\"k shape after applying RoPE: {k.shape}\")  # Should be [batch_size, seq_len, head_size]\n",
    "\n",
    "        # Compute attention scores\n",
    "        wei = torch.bmm(q, k.transpose(-2, -1)) / (q.size(-1) ** 0.5)  # Compute attention scores\n",
    "        wei = wei.masked_fill(self.tril[:T, :T].to(self.device) == 0, float('-inf'))  # Apply the triangular mask\n",
    "        wei = F.softmax(wei, dim=-1)  # Apply softmax to get probabilities\n",
    "        wei = self.dropout(wei)  # Apply dropout\n",
    "        out = torch.bmm(wei, v)  # Compute output as weighted sum of values\n",
    "\n",
    "        return out\n"
   ]
